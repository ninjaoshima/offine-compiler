    <div class="w-full py-4 dark:bg-gray-800" id="features">
      <div class="w-full max-w-6xl px-4 md:px-6 mx-auto my-4 lg:my-6">
        <div class="w-full">
          <h2 class="text-xl font-bold">Online {{languageName}} Compiler</h2>
